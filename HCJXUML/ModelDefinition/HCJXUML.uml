    <comment
      Id="e33ad3e1-17d7-493e-a7cf-e8f1b3217a25">
      <body>SELECT
    --合同编号
    A.ContractCode,
    --工程项目
    A.ProjectName,
    --产品名称
    B.ProductName,
    --产品类型
    C.Text ProductType,
    --型号
    B.Model,
    --规格
    B.Specifications,
    --合同数量
    B.Quantity,
    --零件图号
    E.PartFigureCode,
    --零件名称
    E.PartName
FROM [dbo].[PMS_BN_Project] A
    LEFT JOIN [dbo].[PMS_BN_ProjectDetail] B
        ON A.ProjectID = B.MainID
           AND B.IsEnable = '1'
    LEFT JOIN [HBHC_Sys].[dbo].[sys_code] C
        ON C.CodeType = 'ProductType'
           AND B.ProductType = C.Value
           AND C.IsEnable = '1'
    LEFT JOIN [dbo].[PMS_DesignTaskDetail] D
        ON B.ID = D.ProductID
           AND D.IsEnable = '1'
    LEFT JOIN [dbo].[SYS_BOM] E
        ON D.PartCode = E.PartCode
           AND E.IsEnable = '1'
WHERE A.IsEnable = '1'
      --合同编号
      AND A.ContractCode = '';</body>
