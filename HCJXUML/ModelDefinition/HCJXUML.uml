    A.ID,
    --工序编码
    A.ProcessCode,
    --工序名称
    A.ProcessName,
    --工序类型
    B.Text
FROM [dbo].[PRS_BD_StandardProcess] A
    LEFT JOIN [HBHC_Sys].[dbo].[sys_code] B
        ON A.ProcessType = B.Value
           AND B.CodeType = 'ProcessType'
           AND B.IsEnable = '1'
WHERE A.IsEnable = '1';</body>
