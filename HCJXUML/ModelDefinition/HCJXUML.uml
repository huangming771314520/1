    <comment
      Id="f13e9a38-8b6d-4860-9073-0b2abc12b4b5">
      <body>SELECT A.ContractCode,
       A.ProjectName,
       B.ProductName,
       D.ProductQuantity,
       D.PlanFinishDate
FROM [dbo].[PMS_BN_Project] A
    INNER JOIN [dbo].[PMS_BN_ProjectDetail] B
        ON A.ProjectID = B.MainID
           AND B.IsEnable = '1'
    INNER JOIN [dbo].[PMS_ProductTask] C
        ON B.ID = C.ProductID
           AND C.TaskType = '3'
           AND C.IsEnable = '1'
    INNER JOIN [dbo].[PMS_ProductPlanDetail] D
        ON C.ID = D.MainID
           AND D.IsEnable = '1'
WHERE D.PlanFinishDate &lt;= DATEADD(DAY, -1, DATEADD(MM, DATEDIFF(MM, 0, '') + 1, 0))
      OR
      (
          D.CompleteQuantity &lt; D.ProductQuantity
          AND D.PlanFinishDate &lt; DATEADD(MM, DATEDIFF(MM, 0,''), 0)
      );</body>
      <annotatedElements>
        <opaqueActionMoniker
          Id="9a244220-c19b-4d9e-bb1c-062219f9edbd"
          LastKnownName="查询应交工程明细"
          LastKnownLocation="HCJXUML.uml" />
      </annotatedElements>
    </comment>
