    <comment
      Id="8700e908-fddc-4364-a70b-8ea55afb167b">
      <body>SELECT --合同编号
    a.ContractCode,
    --工程项目
    a.ProjectName,
    --产品名称
    B.ProductName,
    --产品类型
    C.Text ProductType,
    --型号
    B.Model,
    --规格
    B.Specifications,
    --零件图号
    F.PartFigureCode,
    --零件名称
    F.PartName,
    --批产数量
    J.ProductQuantity,
    --交货日期
    J.PlanFinishDate
FROM dbo.PMS_BN_Project a
    INNER JOIN [dbo].[PMS_BN_ProjectDetail] B
        ON a.ProjectID = B.MainID
           AND B.IsEnable = '1'
    INNER JOIN [HBHC_Sys].[dbo].[sys_code] C
        ON C.CodeType = 'ProductType'
           AND B.ProductType = C.Value
           AND C.IsEnable = '1'
    INNER JOIN [dbo].[PMS_ProductTask] H
        ON B.ID = H.ProductID
           AND H.TaskType = '3'
           AND H.IsEnable = '1'
    INNER JOIN [dbo].[PMS_ProductPlanDetail] J
        ON H.ID = J.MainID
           AND J.IsEnable = '1'
    INNER JOIN [dbo].[PMS_ProductTask] D
        ON D.ProductID = B.ID
           AND D.TaskType = '1'
           AND D.IsEnable = '1'
    INNER JOIN [dbo].[PMS_DesignTaskDetail] E
        ON D.ID = E.MainID
           AND E.IsEnable = '1'
    INNER JOIN  [dbo].[PRS_Process_BOM] F
        ON E.PartCode = F.PartCode 
           AND F.IsEnable = '1'
WHERE a.IsEnable = '1'
      AND a.ContractCode = '';</body>
