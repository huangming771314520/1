        <field name='temp.RepairDate' cp='daterange'></field>
        <field name='temp.RepairReason' cp='like'></field>
        <field name='temp.CreateTime' cp='daterange'></field>
      </where>
    </settings>
    <!--配置datagrid的列属性-->
